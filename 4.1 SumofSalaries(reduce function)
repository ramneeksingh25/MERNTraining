salaries=[1000,2000,3000,4000,8000,9000,10000,20000,38000,12000];
const sumofsalaries = salaries.reduce((sum,currentValue)=>sum+((currentValue>=10000)?currentValue:0),0);
//output:80000
const max = salaries.reduce((max,currentValue)=>(max>currentValue?max:currentValue));
  //38000
const count= salaries.reduce((c,i)=>((i>10000)?++c:c),0);
  //3

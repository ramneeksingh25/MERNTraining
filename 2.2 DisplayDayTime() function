function displaydatetime(){
    d=new Date();   
    day=d.getDay();
    Today={1:"Monday",2:"Tueday",3:"Wednesday",4:"Thursday",5:"Friday",6:"Saturday",7:"Sunday"}
    time=d.getHours().toString()+" "+d.getMinutes().toString()+" "+d.getSeconds().toString();
    console.log("Today is:"+Today[day]);
    console.log("Time is:"+time);
}
